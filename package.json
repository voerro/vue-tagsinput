{
  "name": "@voerro/vue-tagsinput",
  "version": "3.0.0",
  "description": "A simple tags input with typeahead made with Vue.js 2",
  "author": "Alexander Zavyalov <alex@voerro.com> (http://voerro.com)",
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name 'VoerroTagsInput' ./src/index.ts",
    "build:demo": "vue-cli-service build"
  },
  "main": "dist/VoerroTagsInput.common.js",
  "buildEntry": "src/",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "public",
    "package.json",
    "tsconfig.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.16.2",
    "@vue/cli": "^4.5.13",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "^4.5.13",
    "bulma": "^0.9.3",
    "cache-loader": "^4.1.0",
    "cross-env": "^7.0.3",
    "sass": "^1.40.1",
    "sass-loader": "^10.2.0",
    "typescript": "^4.4.3",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vue",
    "vuejs",
    "tagsinput",
    "input",
    "tags",
    "typeahead",
    "autocomplete"
  ],
  "license": "MIT",
  "repository": "https://github.com/voerro/vue-tagsinput"
}
